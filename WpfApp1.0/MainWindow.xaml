<Window x:Class="WpfApp1._0.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1._0"
        mc:Ignorable="d"
        Title="Belka version 1.0" Height="765" Width="1150">

    <Window.Resources>
        <local:BeamUnderLoad x:Key="beam"/>
    </Window.Resources>

    <Border Margin="1.5, 0">
        <Grid Language="pl-PL">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>

            <!--//////////////////////////  First column  ////////////////////////// -->
            <StackPanel Grid.Column="0" Margin="1.5">
                <!-- Układ belki -->
                <StackPanel>
                    <Border BorderBrush="DimGray" BorderThickness="0.5" Padding="5" Margin="0, 4">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="2.2*"/>
                            <ColumnDefinition Width="*"/>
                        
                           
                        </Grid.ColumnDefinitions>
                        <StackPanel Grid.Column="0">
                            <TextBlock Text="Układ belki" FontWeight="bold" HorizontalAlignment="Center" Margin="0,2"/>
                            <StackPanel Margin="0 2" Orientation="horizontal">
                                <TextBlock Text="Obciążęnie stałe &#x0394;g=  " VerticalAlignment="center" />
                                    <TextBox Padding="2" Text="{Binding Beam.Loads.DGLoad, Source={StaticResource beam}}" Background="LemonChiffon"/>
                                <TextBlock Text=" kN/m" VerticalAlignment="center"/>
                            </StackPanel>
                            <StackPanel Margin="0 2" Orientation="horizontal">
                                <TextBlock Text="Obciążęnie zmienne q=  " VerticalAlignment="center"/>
                                    <TextBox Padding="2" Text="{Binding Beam.Loads.QLoad, Source={StaticResource beam}}" Background="LemonChiffon"/>
                                <TextBlock Text=" kN/m" VerticalAlignment="center"/>
                            </StackPanel>
                            <StackPanel Margin="0 2" Orientation="horizontal">
                                <TextBlock Text="Rozpiętość belki L=  " VerticalAlignment="center"/>
                                    <TextBox Padding="2" Text="{Binding Beam.Dimensions.Length, Source={StaticResource beam}}" Background="LemonChiffon"/>
                                <TextBlock Text=" m" VerticalAlignment="center"/>
                            </StackPanel>
                        </StackPanel>
                        <StackPanel Grid.Column="1" VerticalAlignment="Center">

                            <Image Width="80" Source="Draws/schemat_statyczny.jpg" VerticalAlignment="Center"/>
                        </StackPanel>
                    </Grid>
                    </Border>
                </StackPanel>

                <!-- Rodzaj materiałów -->
                <StackPanel>
                    <Border BorderBrush="DimGray" BorderThickness="0.5" Padding="5" Margin="0, 0, 0, 4">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="2.8*"/>
                                <ColumnDefinition Width="*"/>

                            </Grid.ColumnDefinitions>
                            <StackPanel Grid.Column="0">
                                <TextBlock Text="Dane podstawowe" FontWeight="bold" HorizontalAlignment="Center" Margin="0,2"/>
                                <StackPanel Margin="0 2" Orientation="Horizontal">
                                    <TextBlock Text="Klasa betonu" VerticalAlignment="Center" />
                                    <ComboBox Margin="20 0" ItemsSource="{Binding Beam.ConcreteParameters.TypesOfConcrete, 
                                        Source={StaticResource beam}}" SelectedIndex="0" 
                                              SelectedItem="{Binding Beam.ConcreteParameters.ConcreteClassDescription, Source={StaticResource beam}}"/>
                                </StackPanel>
                                <StackPanel Margin="0 2" Orientation="Horizontal">
                                    <TextBlock Text="Gatunek stali zbrojeniowej" VerticalAlignment="Center"/>
                                    <ComboBox Margin="20 0" ItemsSource="{Binding Beam.SteelParameters.TypesOfSteel, 
                                        Source={StaticResource beam}}" SelectedIndex="0" 
                                              SelectedItem="{Binding Beam.SteelParameters.SteelClassDescription, Source={StaticResource beam}}"/>
                                </StackPanel>
                                <StackPanel Margin="0 2" Orientation="Horizontal">
                                    <TextBlock Text="Średnica zbroj. podłużnego &#x03C6;" VerticalAlignment="Center"/>
                                    <ComboBox Margin="20 0" ItemsSource="{Binding Beam.SteelParameters.FiArray, 
                                        Source={StaticResource beam}}" SelectedIndex="0" 
                                              SelectedItem="{Binding Beam.SteelParameters.Fi, Source={StaticResource beam}}"/>
                                </StackPanel>
                                <StackPanel Margin="0 2" Orientation="Horizontal">
                                    <TextBlock Text="Średnica strzemin &#x03C6;&#x209B;" VerticalAlignment="Center"/>
                                    <ComboBox Margin="20 0" ItemsSource="{Binding Beam.SteelParameters.FiSArray, 
                                        Source={StaticResource beam}}" SelectedIndex="1" 
                                              SelectedItem="{Binding Beam.SteelParameters.FiS, Source={StaticResource beam}}"/>
                                </StackPanel>
                                <StackPanel Margin="0 2" Orientation="Horizontal">
                                    <TextBlock Text="Typ zbrojenia sprężającego" VerticalAlignment="Center" />
                                    <ComboBox Margin="20 0" ItemsSource="{Binding Beam.PrestressingSteelParameters.TypesOfSteel, 
                                        Source={StaticResource beam}}" SelectedIndex="0" 
                                              SelectedItem="{Binding Beam.PrestressingSteelParameters.PrestressingSteelClassDescription, Source={StaticResource beam}}"/>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Grid.Column="1" VerticalAlignment="Center">
                                <Button Name="Dane"  Width="90" Command="{Binding Beam.ShowMaterialParameters, Source={StaticResource beam}}" CommandParameter="{DynamicResource beam}"
                                        BorderThickness="2">
                                    <Image Source="D:/DEV/C#/Magisterka/WpfApp1.0/WpfApp1.0/Draws/parametry_materialowe2.jpg"/>
                                    <Button.ToolTip>
                                        <ToolTip>
                                            <TextBlock Text="Dane materiałowe"/>
                                        </ToolTip>
                                    </Button.ToolTip>
                                </Button>
                            </StackPanel>
                            
                        </Grid>
                    </Border>
                </StackPanel>

                <!-- Wymiary przekroju -->
