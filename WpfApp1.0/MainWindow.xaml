                            </StackPanel>
                            <StackPanel Margin="0 2" Orientation="horizontal">
                                <TextBlock Text="Utrata siły na skutek poślizgu &#x0394;Ps1= " VerticalAlignment="center"/>
                                <TextBlock Padding="2" Text="{Binding StratyTechnologiczne.DeltaPs1, Mode=OneWay, Source={StaticResource beam}, StringFormat={}{0:n2}}" FontWeight="bold"/>
                                <TextBlock Text=" kN;" VerticalAlignment="center"/>
                            </StackPanel>
                            <StackPanel Margin="0 2" Orientation="horizontal">
                                <TextBlock Text="Siła przejściowa po poślizgu P0.s1= " VerticalAlignment="center"/>
                                <TextBlock Padding="2" Text="{Binding StratyTechnologiczne.P0s1, Mode=OneWay, Source={StaticResource beam}, StringFormat={}{0:n2}}" FontWeight="bold"/>
                                <TextBlock Text=" kN;" VerticalAlignment="center"/>
                            </StackPanel>
                            <StackPanel Margin="0 2" Orientation="horizontal">
                                <TextBlock Text="Procentowa wartość strat technologicznych " VerticalAlignment="center"/>
                                <TextBlock Padding="2" Text="????" FontWeight="bold"/>
                                <TextBlock Text=" %" VerticalAlignment="center"/>
                            </StackPanel>
                        </StackPanel>
                    </Border>
                </StackPanel>

            </StackPanel>

            <!--//////////////////////////  Third column  ////////////////////////// -->
            <StackPanel Grid.Column="2" Margin="1.5">
                <StackPanel>
                    <Border BorderBrush="DimGray" BorderThickness="0.5" Padding="5" >
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="2.8*"/>
                                <ColumnDefinition Width="*"/>

                            </Grid.ColumnDefinitions>
                            <StackPanel Grid.Column="0">
                                <TextBlock Text="Dane podstawowe" FontWeight="bold" HorizontalAlignment="Center" Margin="0,2"/>
                                <StackPanel Margin="0 2" Orientation="Horizontal">
                                    <TextBlock Text="Klasa betonu" VerticalAlignment="Center" />
                                    <ComboBox Margin="20 0" ItemsSource="{Binding Beam.ConcreteParameters.TypesOfConcrete, 
                                        Source={StaticResource beam}}" SelectedIndex="0" 
                                              SelectedItem="{Binding Beam.ConcreteParameters.ConcreteClassDescription, Source={StaticResource beam}}"/>
                                </StackPanel>
                                <StackPanel Margin="0 2" Orientation="Horizontal">
                                    <TextBlock Text="Gatunek stali zbrojeniowej" VerticalAlignment="Center"/>
                                    <ComboBox Margin="20 0" ItemsSource="{Binding Beam.SteelParameters.TypesOfSteel, 
                                        Source={StaticResource beam}}" SelectedIndex="0" 
                                              SelectedItem="{Binding Beam.SteelParameters.SteelClassDescription, Source={StaticResource beam}}"/>
                                </StackPanel>
                                <StackPanel Margin="0 2" Orientation="Horizontal">
                                    <TextBlock Text="Średnica zbroj. podłużnego &#x03C6;" VerticalAlignment="Center"/>
                                    <ComboBox Margin="20 0" ItemsSource="{Binding Beam.SteelParameters.FiArray, 
                                        Source={StaticResource beam}}" SelectedIndex="0" 
                                              SelectedItem="{Binding Beam.SteelParameters.Fi, Source={StaticResource beam}}"/>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Grid.Column="1" VerticalAlignment="Center">
                                <Button Name="Daaane"  Width="40" Command="{Binding Beam.ShowMaterialParameters, Source={StaticResource beam}}" CommandParameter="{DynamicResource beam}">
                                    <Image Source="D:/DEV/C#/Magisterka/WpfApp1.0/WpfApp1.0/Draws/parametry_materialowe.jpg"/>
                                </Button>
                            </StackPanel>

